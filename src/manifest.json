{
  "name": "HabitEC Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "55",
  "description": "N",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/*"
      ],
      "js": [
        "js/content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.amazon.co.jp/*"
  ]
}
